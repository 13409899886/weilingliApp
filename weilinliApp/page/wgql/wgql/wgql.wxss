text{
  font-size:26rpx;
  display: -webkit-box;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
  overflow: hidden;
  text-overflow: ellipsis
}
.iconface{
  padding: 30rpx;
  box-sizing:border-box;

}
.chatWindow .left .content{background:#fff;}
.iconface image{height: 40rpx;width:40rpx; margin-left:2px;}
.chatWindow{ min-height: 100%}
.chatWindow .left .content,.chatWindow .left .userInfo{float: left;}
.chatWindow .right .content,.chatWindow .right .userInfo{float: right}
.chatWindow .right .userInfo,.chatWindow .left .userInfo{text-align: center}
.chatWindow  .content{max-width: 450rpx;position: relative;background: #a2e65b;border: 1px solid #ddd;padding: 15px 15px;margin: 0 15px;border-radius: 2px; text-align: left;word-break:break-all;word-wrap:break-word;position: relative}
.chatWindow  .content img{vertical-align: middle}
/* .chatWindow  .content .rich-text{font-size: 40rpx} */
/* .chatWindow .left,.chatWindow .right{overflow: hidden} */
.chatWindow text.time{text-align: center; padding: 2px 5px; border-radius: 2px; background: #ddd;color: #333;display: inline;margin-bottom: 15px;}
.chatWindow{text-align: center}
.chatWindow .face{height: 40rpx;width: 40rpx; vertical-align: middle}
.chatWindow .left .content::before{
    content: "";
    display: block;
    width: 10px;
    height: 10px;
    border: 1px solid #ddd;
    position: absolute;
    top: 5px;
    left: -6px;
    border-right:none;
    border-bottom:none;
    transform: skew(45deg);
    background: #fff;
  }
  .chatWindow .right .content::before{
    content: "";
    display: block;
    width: 10px;
    height: 10px;
    border: 1px solid #ddd;
    position: absolute;
    top: 5px;
    right: -6px;
    border-left:none;
    border-bottom:none;
    transform: skew(-45deg);
    background: #a2e65b;
    
  }
  .chatObj{padding: 15px 15px 0; overflow: hidden}
  .chatObj>view{margin-top: 15px}
  .lv_tabber button{width: 70px;height:60rpx; line-height: 60rpx;padding: 0 5px}
  .content{font-size: 36rpx;min-height:36rpx}
  .swiper view{background: #fff}
  .comback{background: #fff;padding: 10px 15px; line-height: 2; position: fixed; top: 0;left: 0; right:0;z-index: 100;display: flex; justify-content: space-between; align-items: center }
  .comback::before{
        content: " ";
        position: absolute;
        left: 0;
        top: 0;
        right: 0;
        height: 1px;
        border-top: 1px solid #E5E5E5;
        color: #E5E5E5;
        -webkit-transform-origin: 0 0;
        transform-origin: 0 0;
        -webkit-transform: scaleY(0.5);
        transform: scaleY(0.5);
  }
  .lv_tabber .clVideoBtn{width: 480rpx}
  .btnContainer{
    position: absolute;
    left: 0;top: 0;
    transform: translateY( -100%);
    z-index:1000;
    width: 150px;
    background: rgba(255,255,255,0)
  }
  .right .btnContainer{
    left: initial;
    right: 0;
    text-align: right;
  }
  .right .btnContainer button{
    font-size: 12px;
    padding: 0 5px
  }
   .btnContainer button+button{
    margin-left: 5px
  }
  .comback .iconfont{font-size: 40rpx !important}
  .comback{line-height: 1}
  .content .img{
    max-width: 100%
  }