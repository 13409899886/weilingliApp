/* page/wgql/wxyAuditor.wxss */

page{height: 100%}
view{background: inital}
.content{min-height: 100%}
.userList{
  background: #fff;
  padding: 30rpx 0 ;
  overflow: hidden;
  padding-top: 0;
  position: relative
 }
.userList .obj{
  width: 120rpx;
  overflow: hidden;
  margin-left: 25rpx;
  float: left;
  margin-top: 30rpx;
  background: #fff;
}
.userName{
  overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    word-wrap: normal;
    word-wrap: break-word;
    word-break: break-all;
    background: #fff;
    text-align: center;
    font-size: 12px
}
.userList .obj .picture{
  width: 120rpx;
  height: 120rpx;
  border: 1rpx solid #e8e8e8;
  border-radius: 5px;
  background-repeat: no-repeat;
  background-size: cover;
  position: relative; 
  overflow: hidden;
  box-sizing: border-box;
  background-position: center center;
}
.userList .obj .picture .text{
  position: absolute;
  left:0;
  right: 0;
  bottom: 0;
  line-height: 1.5;
  background: #fff;
  font-size: 12px;
  text-align: center; 
  box-sizing: border-box;
}
.weui-panel {
    background-color: #FFFFFF;
    margin-top: 10px;
    position: relative;
    overflow: hidden;
}
.weui-media-box,.weui-media-box_small-appmsg {
    padding: 0;
}
.weui-media-box {
    
    position: relative;
}
.weui-media-box_small-appmsg .weui-cells {
    margin-top: 0;
}
.weui-cells {
    margin-top: 1.17647059em;
    background-color: #FFFFFF;
    line-height: 1.47058824;
    font-size: 17px;
    overflow: hidden;
    position: relative;
}
.weui-cell_access {
    -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
    color: inherit;
    background: #fff
}
.weui-cell_access view{background: rgba(0,0,0,0)}
.weui-cell {
    padding: 10px 15px;
    position: relative;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-align: center;
    -webkit-align-items: center;
    align-items: center;
}
.weui-cell__bd {
    -webkit-box-flex: 1;
    -webkit-flex: 1;
    flex: 1;
}
.weui-cell_primary {
    -webkit-box-align: start;
    -webkit-align-items: flex-start;
    align-items: flex-start;
}
.weui-cell_access .weui-cell__ft {
    padding-right: 13px;
    position: relative;
}
.weui-cell__ft {
    text-align: right;
    color: #808080;
}
.weui-cell_access .weui-cell__ft:after {
    content: " ";
    display: inline-block;
    height: 6px;
    width: 6px;
    border-width: 2px 2px 0 0;
    border-color: #C8C8CD;
    border-style: solid;
    -webkit-transform: matrix(0.71, 0.71, -0.71, 0.71, 0, 0);
    transform: matrix(0.71, 0.71, -0.71, 0.71, 0, 0);
    position: relative;
    top: -2px;
    position: absolute;
    top: 50%;
    margin-top: -4px;
    right: 2px;
}
.weui-media-box,.weui-panel {
  background: #fff
}
.weui-cell_access:active,.weui-cell_access view:active {
    background-color: #ECECEC;
}
.weui-cells:after,.userList:before {
    content: " ";
    position: absolute;
    left: 0;
    bottom: 0;
    right: 0;
    height: 1px;
    border-bottom: 1px solid #e5e5e5;
    color: #e5e5e5;
    -webkit-transform-origin: 0 100%;
    transform-origin: 0 100%;
    -webkit-transform: scaleY(0.5);
    transform: scaleY(0.5);
    z-index: 2;
}
.weui-cell:before {
    content: " ";
    position: absolute;
    left: 0;
    top: 0;
    right: 0;
    height: 1px;
    border-top: 1px solid #e5e5e5;
    color: #e5e5e5;
    -webkit-transform-origin: 0 0;
    transform-origin: 0 0;
    -webkit-transform: scaleY(0.5);
    transform: scaleY(0.5);
    left: 15px;
    z-index: 2;
}
.weui-cell:first-child:before{
  left: 0
}
.notice .weui-cell_primary{
  display: block;
}
.notice .noticeDetail{
  font-size: 24rpx;color: #666
}
.lookMore .picture{
  display: flex;
  align-items: center;
  text-align: center;
  background:#fff
}
.weui-mask {
    position: fixed;
    z-index: 1000;
    top: 0;
    right: 0;
    left: 0;
    bottom: 0;
    background: rgba(0, 0, 0, 0.6);
}
.weui-mask .codePic{
  width: 100%;
  position: absolute;
  top: 50%;
  transform: translateY(-50%)
}